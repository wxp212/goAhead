{
    "name": "exp-js",
    "description": "Js Plugin for Expansive",
    "version": "0.2.4",
    "keywords": [
        "expansive-plugin",
        "js"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-js.git"
    },
    "devDependencies": {
        "pak": ">=0.11.3"
    },
    "pak": {
        "scripts": {
            "postcache": {
                "script": "if (!Cmd.locate('uglifyjs')) { if (!Cmd.locate('npm')) { throw 'Cannot locate the \"npm\" utility.' } else { run('npm install -g uglify-js')}}"
            }
        },
        "origin": "embedthis/exp-js"
    }
}
